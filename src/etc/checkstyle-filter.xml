<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
    "-//Puppy Crawl//DTD Suppressions 1.1//EN"
    "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">

<!-- checks="[a-zA-Z0-9]*" matches all checks -->
<suppressions>
	<!-- No package-javadoc for JUnit-Tests needed -->
    <suppress files=".*Test\.java" checks="JavadocPackage" />
    <suppress files="[\\/]testclasses[\\/]" checks="JavadocPackage" />
    <!-- Don't check for magic Numbers in JUnit -->
    <suppress files="[\\/]testclasses[\\/]" checks="MagicNumber" />
    <suppress files=".*Test\.java" checks="MagicNumber" />
    <suppress files=".*NodeHelper\.java" checks="MethodCount" />
</suppressions>
